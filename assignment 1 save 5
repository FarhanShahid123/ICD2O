package javaapplication6;
import java.lang.Math;
import java.util.Scanner;
/**
 *
 * @author 787462
 */
public class JavaApplication6 {

    /**8
     *
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int length;
        int width;
        Scanner sc = new Scanner (System.in);
        System.out.println("Length is");
        width = sc.nextInt();
        System.out.println ("Width is");
        length = sc.nextInt();
        System.out.println ("This is the area");
        System.out.println(length * width);

        double radius;
        System.out.println("whats the radius ");
        radius = sc.nextDouble();
        System.out.println (Math.PI*(radius*radius));
        int height;    
        double Radius;
        System.out.println ("Whats the height ");
        height = sc.nextInt ();
        System.out.println ("whats the radius ");
        Radius = sc.nextInt ();
        System.out.println (Math.PI*(radius*radius)*height);
        int L;
        double G;
        G = 9.8;
        System.out.print("input meters: ");
        L = sc.nextInt();
        System.out.println((Math.PI*Math.PI)*Math.sqrt(L/G));
                
    }
}
